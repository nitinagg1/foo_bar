<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ServeSpeed" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ServeSpeed.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayVideoSelect"
            android:label="Video Player"
         />
        <activity android:name=".ServeCoordinates"
            android:label="Serve Coordinates"/>
        <activity android:name=".BounceCoordinates"
            android:label="bounce coordinates"/>
        <activity android:name=".DisplaySpeed"
            android:label="displaying speed"/>
        <activity android:name=".DisplayScreenShot"
            android:label="displaying screenshot"/>
        <activity android:name=".PlayVideoSelect2"
            android:label="Video Player2"
            />
        <activity android:name=".ServePosition"
            android:label="serveposition"
            />
        <activity android:name=".ServePosition2"
            android:label="serveposition2"
            />
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

</manifest>
